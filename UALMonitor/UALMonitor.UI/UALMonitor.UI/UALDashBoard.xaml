<Window x:Class="UALMonitor.UI.UALDashBoard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UALDashBoard" Height="800" Width="1000">
    <Grid>
        <ListBox HorizontalAlignment="Left" Height="118" Margin="10,10,0,0" VerticalAlignment="Top" Width="199"/>
        <DataGrid HorizontalAlignment="Left" Margin="10,133,0,0" VerticalAlignment="Top" Height="627" Width="461"/>
        <RichTextBox HorizontalAlignment="Left" Height="750" Margin="476,10,0,0" VerticalAlignment="Top" Width="506">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
